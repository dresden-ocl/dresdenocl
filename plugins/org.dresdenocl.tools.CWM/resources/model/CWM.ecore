          </eStructuralFeatures>
          <eStructuralFeatures xsi:type="ecore:EReference" name="widerTerm" upperBound="-1" eType="#//cwm/analysis/businessnomenclature/Term" eOpposite="#//cwm/analysis/businessnomenclature/Term/narrowerTerm">
